{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "typeRoots": [
      "./custom_typings",
    ],
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}